.Top {
  position: fixed;
  z-index: 100;
  top: 0px;
  width: 750rpx;
}

.search {
  width: 100%;
  border-radius: 30rpx;
  background-color: white;
}

.img {
  width: 220rpx;
  height: 252rpx;
}

.dizhi {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.to_top{
  position: fixed;
  top: 60%;
  right: 0;
  z-index: 100;
}

.midt {
  position: absolute;
  width: 100vw;
  z-index: 10;
  bottom: 0%;
  /* opacity: 0.8; */
  box-shadow: 1px 1px 5px #dfdfdf;
}
.mid {
  width: 85.5vw;
}

.biaoqian {
  min-width: 56rpx;
  height: 38rpx;
  line-height: 38rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
}

.li {
  width: 100%;
  height: 312rpx;
}

.li1 {
  width: 600rpx;
  margin: 0 auto;
  border-radius: 10rpx;
  height: 300rpx;
  transform: scale(0.9);
}

.selected {
  transform: scale(1);
}

.search_icon {
  width: 35rpx;
  height: 30rpx;
}

