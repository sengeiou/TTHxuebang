          </cover-view>

        </cover-view>

      </cover-view>

    </view>

    <!-- <view wx:if='{{model!="iPhone X "&&model!="iPhone XR "&&model!="iPhone XS "&&model!="iPhone XS Max "}}' style='height:280rpx;'></view>

