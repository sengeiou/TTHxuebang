            </cover-view>
            <cover-view wx:if="{{jginfo.scoring==4}}" class='flex-row flex-center margin-right-20'>
              <cover-image wx:for='{{4}}' src='{{uploadpath}}resource/{{res.star}}' class='margin-left-5' style='height:24rpx;width:24rpx;'></cover-image>
