<!--pages/myorder/myorder.wxml-->
<view class=' flex-row flex-center  bg-white bor_bot top'>

  <view class='flex-1 flex-row flex-column' bindtap='bindshow' data-type='all'>
    <view class='  flex-1 h5 padding-10 margin-bottom-5 margin-top-10 {{show=="all"?"txt-bold":""}} txt-black'>全部</view>
    <view wx:if='{{show=="all"}}' class='bg-primary' style='width:120rpx;height:6rpx;'></view>
    <view wx:if='{{show!="all"}}' class='' style='width:80rpx;height:6rpx;'></view>
  </view>

  <view class='flex-1 flex-row flex-column' bindtap='bindshow' data-type='wc'>
    <view class='  flex-1 h5 padding-10 margin-bottom-5 margin-top-10 {{show=="wc"?"txt-bold":""}} txt-black'>已完成</view>
    <view wx:if='{{show=="wc"}}' class='bg-primary' style='width:120rpx;height:6rpx;'></view>
    <view wx:if='{{show!="wc"}}' class='' style='width:80rpx;height:6rpx;'></view>
  </view>

  <view class='flex-1 flex-row flex-column' bindtap='bindshow' data-type='df'>
    <view class='  flex-1 h5 padding-10 margin-bottom-5 margin-top-10 {{show=="wait"?"txt-bold":""}} txt-black'>待付款</view>
    <view wx:if='{{show=="wait"}}' class='bg-primary' style='width:120rpx;height:6rpx;'></view>
    <view wx:if='{{show!="wait"}}' class='' style='width:80rpx;height:6rpx;'></view>
  </view>

  <view class='flex-1 flex-row flex-column' bindtap='bindshow' data-type='dsh'>
    <view class='  flex-1 h5 padding-10 margin-bottom-5 margin-top-10 {{show=="dsh"?"txt-bold":""}} txt-black'>待收货</view>
    <view wx:if='{{show=="dsh"}}' class='bg-primary' style='width:120rpx;height:6rpx;'></view>
    <view wx:if='{{show!="dsh"}}' class='' style='width:80rpx;height:6rpx;'></view>
  </view>

  <view class='flex-1 flex-row flex-column' bindtap='bindshow' data-type='dpj'>
    <view class='  flex-1 h5 padding-10 margin-bottom-5 margin-top-10 {{show=="dpj"?"txt-bold":""}} txt-black'>待评价</view>
    <view wx:if='{{show=="dpj"}}' class='bg-primary' style='width:120rpx;height:6rpx;'></view>
    <view wx:if='{{show!="dpj"}}' class='' style='width:80rpx;height:6rpx;'></view>
  </view>

  <!-- <view class='xiaoxi'>
    <view wx:if="{{dflist.length>0}}" class='waitpay '>{{dflist.length}}</view>
  </view> -->

</view>




<view style='height:88rpx;'></view>

<view class='' wx:if='{{show=="all"}}'>

  <view wx:if='{{alllist.length==0}}' class="flex-row flex-column">
    <view style="height:100rpx;"></view>
    <image src="{{uploadpath}}resource/{{res.dindannull}}" class="icon-170 "></image>

  </view>
  <view class="margin-24 order" wx:for="{{alllist}}" wx:key='k'>
    <view bindtap="courseinfo" data-id="{{item.course_id}}" class="padding-20 flex-row ">

      <image wx:if="{{item.type!='SP'}}" class='icon radius-10' lazy-load='{{true}}' src='{{uploadpath}}curriculum/{{item.course_kc_img}}'></image>

      <image wx:if="{{item.type=='SP'}}" class='icon radius-10' lazy-load='{{true}}' src='{{uploadpath}}onlineclassroom/{{item.onlineclassroom_img}}'></image>

      <view class="margin-left-20 flex-1">
        <view class="flex-row flex-center ">
          <view wx:if="{{item.type!='SP'}}">{{item.course_name}}</view>
          <view wx:if="{{item.type=='SP'}}">{{item.onlineclassroom_name}}</view>

          <view class="flex-1"></view>
          <image wx:if="{{item.pstatus=='P'||item.pstatus=='U'||item.pstatus=='R'||item.pstatus=='PJ'}}" class='yiwanchen ' lazy-load='{{true}}' src='{{uploadpath}}resource/{{res.yiwanchen}}'></image>
          <view class="h6 " style="color:#FF5252" wx:if="{{item.pstatus=='W'||item.pstatus=='PT'||item.pstatus=='C'}}">{{item.pstatus_name}}</view>
        </view>

        <view wx:if="{{item.type!='SP'}}" class=' txt-gray f-22 margin-top-5  '>{{item.course_jigouname}}</view>
        <view wx:if="{{item.type=='SP'}}" class=' txt-gray f-22 margin-top-5  height-40 '>{{item.onlineclassroom_class_introduce }}</view>

        <view wx:if="{{item.type!='SP'}}" class='flex-row flex-center  margin-top-5' style=''>

          <view class='flex-row flex-center '>
            <image src='{{uploadpath}}resource/{{res.age_icon}}' class='icon-10'></image>
            <view class='h6 txt-black margin-left-5'>{{item.course_age_name}}</view>
          </view>

          <view style='width:20rpx;'></view>

          <view class='flex-row flex-center '>
            <image src='{{uploadpath}}resource/{{res.keshi}}' class='icon-10'></image>
            <view class='h6 txt-black margin-left-5'>{{item.course_class_hours}}</view>
          </view>

          <view style='width:20rpx;'></view>


          <view class='flex-row flex-center '>
            <image src='{{uploadpath}}resource/{{res.time}}' class='icon-10'></image>
            <view class='h6 txt-black margin-left-5'>{{item.course_duration}}</view>
          </view>


        </view>
        <view wx:if="{{item.type=='SP'}}" class='flex-row flex-center  margin-top-5' style=''>

          <view class='flex-row flex-center '>
            <image src='{{uploadpath}}resource/{{res.nianlin}}' class='icon-10'></image>
            <view class='h6 txt-black margin-left-5'>{{item.onlineclassroom_age_name}}</view>
          </view>

          <view style='width:20rpx;'></view>

          <view class='flex-row flex-center '>
            <image src='{{uploadpath}}resource/{{res.zhanjie}}' class='icon-10'></image>
            <view class='h6 txt-black margin-left-5'>{{item.onlineclassroom_chapter_num}}</view>
          </view>

          <view style='width:20rpx;'></view>




        </view>
        <view class="hkg ">
          <view class='flex-row margin-top-10 flex-center'>
            <view wx:if="{{item.type=='PT'}}" class='pt'>拼团</view>
            <view class="flex-1"></view>
          </view>
        </view>
        <view class="flex-row flex-center ">
          <view class="flex-1"></view>
          <view class="f-22">共1个课程 实付款:</view>
          <view class="h5 txt-bold" style="color:#FF6600">￥{{item.amount}}</view>
        </view>


      </view>

    </view>
    <view class="padding-20" style="padding-top:0rpx;">

      <view class="flex-row flex-center padding-top-20 xian">
        <image wx:if="{{item.pstatus=='P'}}" bindtap="shanchu" data-id="{{item.id}}" src='{{uploadpath}}resource/{{res.lajiton}}' class='icon-30'></image>

        <view class="flex-1"></view>
        <block wx:if="{{item.pstatus=='W'}}">
          <view class='txt-gray text-center h6 fbtn' style='border:1px solid #AAAAAA;' id='{{item.id}}' bindtap='colseorder'>取消订单</view>
          <view class=' text-center  margin-left-20 h6 fbtn  ljzf' bindtap='bindpay' id='{{item.id}}'>立即支付</view>
        </block>
        <block wx:if="{{item.pstatus=='P'||item.pstatus=='PT'||item.pstatus=='PJ'}}">
          <view wx:if="{{item.pstatus=='P'&&item.type!='SP'}}" class='txt-gray text-center h6 fbtn' style='border:1px solid #AAAAAA;' id='{{item.id}}' bindtap='pinjialist' data-id="{{item.course_id}}">查看我的评价</view>

          <view wx:if="{{item.pstatus=='PJ'&&item.type!='SP'}}" class='txt-gray text-center h6 fbtn' style='border:1px solid #AAAAAA;' id='{{item.id}}' data-id="{{item.course_id}}" bindtap='pinjia'>评价晒图得50积分</view>
          <view wx:if="{{item.type=='PT'}}" class=' text-center  margin-left-20 h6 fbtn  cktxq' data-pt='{{item.spellgroup_id}}' bindtap='kantuan' id='{{item.id}}'>查看团详情</view>
        </block>
      </view>


    </view>


  </view>
</view>
