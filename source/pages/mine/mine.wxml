<view class='orangeblock'>


  <view style="position: absolute;right: 14rpx;top: 24rpx;">

    <button class='clear-button' hover-class="hover" open-type="getPhoneNumber" bindgetphonenumber="getPhoneNo" style='width:120rpx;height:50rpx;position: absolute;z-index: 10;'>
    </button>

    <view class='gengxin '>
      <view class="flex-row flex-center">
        <image src='{{uploadpath}}resource/{{res.shuaxin}}' class='icon-10'></image>
        <view class='txt-white h7 margin-left-4'>更新资料</view>
      </view>
    </view>

  </view>


  <view class='flex-row flex-center'>

    <view class='flex-row column flex-1'>
      <view class='f48 txt-bold txt-white chaochu'>Hi,{{memberinfo.nickName}}</view>
      <view wx:if='{{memberinfo.mobile!=""&&memberinfo.mobile!="undefined"}}' class='f26 txt-bold txt-white margin-top-10'>{{mobile}}</view>
      <view wx:if='{{memberinfo.mobile==""||memberinfo.mobile=="undefined"}}' class='f26 txt-bold txt-white margin-top-10'>暂无手机号码，请更新资料</view>

      <view class='bianji flex-row flex-center margin-top-20'>
        <view class='flex-1'></view>
        <view bindtap="gotohaizi" class='txt-orange h6'>孩子信息编辑</view>
        <image src='{{uploadpath}}resource/{{res.youjiaobiao}}' class='icon-10 margin-left-4'></image>
        <view class='flex-1'></view>
      </view>

    </view>

    <image class='tx radius-50  ' style=' ' src='{{memberinfo.avatarUrl}}'></image>

  </view>
</view>



