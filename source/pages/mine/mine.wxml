<view class='bg-white radius-8 margin-20'>

  <view class='flex-row flex-center padd-order bor_bot' data-name='dd' bindtap='todetails'>
    <view class='flex-1 txt-black f-26 txt-bold'>我的订单</view>
    <view class='f22'>全部订单</view>
    <image class='icon-15 margin-left-4' src='{{uploadpath}}resource/{{res.to}}'></image>
  </view>

  <view class='padd-order-under flex-row flex-center'>

    <view bindtap="orderlist" data-id='dfk' class='flex-row flex-column flex-1'>
      <view wx:if="{{memberinfo.dfkorder>0}}" class="shuzi">{{memberinfo.dfkorder}}</view>
      <image class='icon-48 ' src='{{uploadpath}}resource/{{res.dddaifukuan}}'></image>
      <view class='h6 margin-top-10'>待付款</view>
    </view>

    <view bindtap="pintuan" data-id='ygm' class='flex-row flex-column flex-1'>
      <view wx:if="{{memberinfo.ypborder>0}}" class="shuzi">{{memberinfo.ypborder}}</view>
      <image class='icon-48' src='{{uploadpath}}resource/{{res.dddaipinban}}'></image>
      <view class='h6 margin-top-10'>待拼班</view>
    </view>

    <view bindtap="orderlist" data-id='dpj' class='flex-row flex-column flex-1'>
      <view wx:if="{{memberinfo.dpjorder>0}}" class="shuzi">{{memberinfo.dpjorder}}</view>
      <image class='icon-48' src='{{uploadpath}}resource/{{res.dddaipinjia}}'></image>
      <view class='h6 margin-top-10'>待评价</view>
    </view>

    <view bindtap="orderlist" data-id='dsh' class='flex-row flex-column flex-1'>
      <view wx:if="{{memberinfo.dshorder>0}}" class="shuzi">{{memberinfo.dshorder}}</view>
      <image class='icon-48' src='{{uploadpath}}resource/{{res.dddaishouhuo}}'></image>
      <view class='h6 margin-top-10'>待收货</view>
    </view>

    <view bindtap="tuikuan" class='flex-row flex-column flex-1'>
      <image class='icon-48' src='{{uploadpath}}resource/{{res.shouhuo}}'></image>
      <view class='h6 margin-top-10'>退款/售后</view>
    </view>

  </view>



</view>


<view class='bg-white margin-20 radius-8'>

  <view data-name='ketan' bindtap='todetails' class='flex-row flex-center margin-30   pd'>
    <image class='icon-24' src='{{uploadpath}}resource/{{res.yigouketan}}'></image>
    <view class='margin-left-20  txt-black  flex-1 h'>已购课堂</view>
    <image class='icon-15' src='{{uploadpath}}resource/{{res.to}}'></image>
  </view>


  <view data-name='xx' bindtap='todetails' class='flex-row flex-center margin-30   pd'>
    <image class='icon-24' src='{{uploadpath}}resource/{{res.myxiaoxi}}'></image>
    <view class='margin-left-20  txt-black  flex-1 h'>我的消息</view>
    <view wx:if="{{xiaoxi>0}}" class="hondian margin-right-20"></view>
    <image class='icon-15' src='{{uploadpath}}resource/{{res.to}}'></image>
  </view>

  <view data-name='dizhi' bindtap='todetails' class='flex-row flex-center margin-30   pd'>
    <image class='icon-24' src='{{uploadpath}}resource/{{res.myaddress}}'></image>
    <view class='margin-left-20  txt-black  flex-1 h'>地址管理</view>
    <image class='icon-15' src='{{uploadpath}}resource/{{res.to}}'></image>
  </view>



  <view data-name='pc' bindtap='todetails' class='flex-row flex-center margin-30   pd'>
    <image class='icon-24' src='{{uploadpath}}resource/{{res.mypingce}}'></image>
    <view class='margin-left-20  txt-black  flex-1 h'>我的评测</view>
    <image class='icon-15' src='{{uploadpath}}resource/{{res.to}}'></image>
  </view>



  <view data-name='wt' bindtap='todetails' class='flex-row flex-center margin-30   pd'>
    <image class='icon-24' src='{{uploadpath}}resource/{{res.chanjian}}'></image>
    <view class='margin-left-20  txt-black  flex-1 h'>常见问题</view>
    <image class='icon-15' src='{{uploadpath}}resource/{{res.to}}'></image>
  </view>


  <view data-name='sc' bindtap='todetails' class='flex-row flex-center margin-30   pd'>
    <image class='icon-24' src='{{uploadpath}}resource/{{res.myfav}}'></image>
    <view class='margin-left-20  txt-black  flex-1 h'>我的收藏</view>
    <image class='icon-15' src='{{uploadpath}}resource/{{res.to}}'></image>
  </view>

  <view data-name='kf' bindtap='todetails' class=' margin-30   pd'>

    <view class='flex-row flex-center'>
      <image class='icon-24' src='{{uploadpath}}resource/{{res.lianxikefu}}'></image>
      <view class='margin-left-20  txt-black h '>联系客服</view>
      <view class='margin-left-40  txt-gray h6 flex-1 '>工作日 {{instinfo.openning}}</view>
      <image class='icon-15' src='{{uploadpath}}resource/{{res.to}}'></image>
    </view>

  </view>


  <view data-name='wm' bindtap='todetails' class='flex-row flex-center margin-30   pd'>
    <image class='icon-24' src='{{uploadpath}}resource/{{res.us}}'></image>
    <view class='margin-left-20  txt-black  flex-1 h'>关于我们</view>
    <image class='icon-15' src='{{uploadpath}}resource/{{res.to}}'></image>
  </view>


  <view data-name='jg' bindtap='todetails' class='flex-row flex-center margin-30   pd'>
    <image class='icon-24' src='{{uploadpath}}resource/{{res.ruzhu}}'></image>
    <view class='margin-left-20  txt-black  flex-1 h'>机构入驻</view>
    <image class='icon-15' src='{{uploadpath}}resource/{{res.to}}'></image>
  </view>

  <view data-name='tg' bindtap='todetails' class='flex-row flex-center margin-30   pd'>
    <image class='icon-24' src='{{uploadpath}}resource/{{res.tuiguang}}'></image>
    <view class='margin-left-20  txt-black  flex-1 h'>大使中心</view>
    <image class='icon-15' src='{{uploadpath}}resource/{{res.to}}'></image>
  </view>


  <view class='margin-top padding' wx:if="{{memberinfo.canhexiao=='Y'}}">
    <button bindtap='startscan'>核销</button>
  </view>

</view>

